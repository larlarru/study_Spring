    </environment>
  </environments>
  <mappers>
    <mapper resource="kr/or/ddit/config/mybatis/mapper/member.xml"/>
    <mapper resource="kr/or/ddit/config/mybatis/mapper/membersMapper.xml"/>
  </mappers>
</configuration>